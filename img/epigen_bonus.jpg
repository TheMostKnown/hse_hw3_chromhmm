/*# sourceMappingURL=data:application/json;base64, */</style><style>.FCPanel {
  --add-height: 36px;
  --collapse-hover-delay: 0.15s;
  --collapse-duration: 0.24s;
  position: fixed;
  right: 26px;
  bottom: 0;
  width: 52px;
  isolation: isolate;
}

.FCPanel__widthToggle {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  width: 32px;
  height: 32px;
  background: none;
  border: 0;
  position: absolute;
  bottom: 100%;
  right: 0;
  cursor: pointer;
  opacity: 0.7;
  transition: opacity 0.2s ease;
  z-index: 1; /* overrides collapse force field */
}

.FCPanel__widthToggleIcon {
  width: 20px;
  height: 20px;
  fill: var(--icon_outline_secondary);
  animation-name: meFCEWidthToggle;
  animation-iteration-count: 1;
  animation-duration: 0.3s;
  animation-fill-mode: backwards;
  animation-direction: reverse;
}

/* перезапускает анимацию */
.FCPanel__widthToggle:active .FCPanel__widthToggleIcon {
  animation-name: none;
}

.FCPanel__widthToggle:hover,
.FCPanel__widthToggle:focus {
  opacity: 1;
}

.FCPanel__widthToggle:focus {
  outline: none;
}

.FCPanel__list {
  background: var(--background_content);
  box-shadow: 0 0 0 1px var(--separator_alternate);
  border-radius: 8px 8px 0 0;
  padding-bottom: var(--add-height);
  transform: translateY(0);
  transition: transform var(--collapse-duration) ease-in;
}

.FCPanel__add {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: var(--add-height);
  background: var(--background_content);
  border: 0;
  position: absolute;
  bottom: 0;
  outline: none;
  color: var(--icon_outline_medium);
  cursor: pointer;
  z-index: 0;
}

/* используем вместо непрозрачной заливки */
.FCPanel__add::before {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  border-radius: inherit;
  background: transparent;
  transition: background 0.2s ease, opacity 0.2s;
  z-index: -1;
}

.FCPanel__add:hover::before,
.FCPanel__add:focus::before {
  background: var(--background_hover);
}

.FCPanel__list:empty + .FCPanel__add {
  border-radius: 8px 8px 0 0;
}

.FCPanel__addIcon {
  opacity: 0.7;
  animation-name: meFCListOpenToggle;
  animation-duration: 0.16s;
  animation-iteration-count: 1;
}

.FCPanel__add:hover .FCPanel__addIcon,
.FCPanel__add:focus .FCPanel__addIcon {
  opacity: 1;
}

.FCPanel__add {
  font-family: inherit;
}

.FCPanel__addText {
  font-size: 12.5px;
  line-height: 16px;
  color: var(--text_secondary);
}

/* Режим открытого списка */
/* перезапускает анимацию */
.FCPanel__add:active .FCPanel__addIcon {
  animation-name: none;
  transform: scale(0.7);
  opacity: 0.15;
  transition-duration: 0.1s;
}

@keyframes meFCListOpenToggle {
  0% {
    transform: scale(0.7);
    opacity: 0.15;
  }
  100% {
    transform: scale(1);
  }
}

/* Развернутый режим */
.FCPanel--wide {
  width: 148px;
}

.FCPanel--wide .FCPanel__widthToggleIcon {
  animation-fill-mode: forwards;
  animation-direction: normal;
  transform: scaleX(-1) translateX(0);
}

.FCPanel--wide .FCPanel__add {
  justify-content: flex-start;
}

.FCPanel--wide .FCPanel__addIcon {
  margin: 0 10px 0 18px;
}

@keyframes meFCEWidthToggle {
  0% {
    transform: scaleX(1) translateX(0);
  }
  50% {
    transform: scaleX(1) translateX(-12px);
    opacity: 0.1;
  }
  50.1% {
    transform: scaleX(-1) translateX(-12px);
    opacity: 0.1;
  }
  100% {
    transform: scaleX(-1) translateX(0);
  }
}

/* Свернутый режим */
.FCPanel--collapsed {
  pointer-events: none;
}

/* Вручную управляем интерактивными областями в свернутом режиме,
 * так как трансформ оставляет родителя на месте */
.FCPanel--collapsed .FCPanel__widthToggle,
.FCPanel--collapsed .FCPanel__list,
.FCPanel--collapsed .FCPanel__add {
  pointer-events: auto;
}

/* Сворачиваем башню */
.FCPanel--collapsed .FCPanel__list {
  transform: translateY(calc(100% - var(--add-height)));
  transition-timing-function: ease-out;
}

/* Дополнительный задел высотой в один элемент,
 * ловит ховер на список и предотвращает сворачивание панельки
 * при удалении первого элемента списка в свернутом режиме
 */
.FCPanel--collapsed .FCPanel__list::before {
  position: absolute;
  bottom: 100%;
  width: 100%;
  padding-top: 100%; /* считаем, что элемент списка квадратный */
  z-index: -1;
}

.FCPanel--collapsed .FCPanel__list:hover::before {
  content: '';
}

/* Тьюнинг задержки скругления уголков на разворачивании */
.FCPanel--collapsed .FCPanel__add {
  transition: border-radius 0s ease calc(var(--collapse-hover-delay) + 0.01s);
}

/* Тьюнинг задержки скругления уголков на сворачивании */
.FCPanel--collapsed:not(:hover) .FCPanel__add {
  border-radius: 8px 8px 0 0;
  transition-delay: 0.05s;
}

/* Разворачиваем по ховеру, но с небольшой защитой от проведения мимо */
.FCPanel--collapsed:hover .FCPanel__list {
  transform: translateY(0);
